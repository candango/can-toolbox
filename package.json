{
  "name": "faz",
  "version": "0.0.4",
  "description": "Candango Faz Javascript toolkit.",
  "main": "faz.js",
  "repository": "https://github.com/candango/faz.git",
  "author": "Flavio Garcia <piraz@candango.org>",
  "license": "Apache-2.0",
  "private": false,
  "dependencies": {
    "bootstrap": "4.3.1",
    "can": "5.25.0",
    "jquery": "3.4.1",
    "less": "3.0.4",
    "popper.js": "1.15.0"
  },
  "scripts": {
    "run": "http-server",
    "build": "node build.js",
    "test": "npm run test:browser",
    "test:browser": "testee test/*.html --config=testee.json"
  },
  "keywords": [
    "canjs",
    "bootstrap",
    "toolkit"
  ],
  "devDependencies": {
    "http-server": "0.11.1",
    "qunit": "2.9.2",
    "steal": "2.2.1",
    "steal-less": "1.3.4",
    "steal-stache": "4.1.3",
    "steal-qunit": "2.0.0",
    "steal-tools": "2.2.2",
    "rollup": "1.12.1",
    "testee": "0.9.1"
  },
  "steal": {
    "plugins": [
      "steal-less",
      "steal-stache"
    ]
  }
}
